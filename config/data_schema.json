{
  "description": "前后端数据接口定义",
  "version": "1.0",

  "price_trend": {
    "description": "时间序列房价趋势数据",
    "format": {
      "months": ["2023-01", "2023-02", "..."],
      "prices": [55000, 54800, "..."],
      "mom_change": [0, -0.36, "..."],
      "yoy_change": [0, -2.5, "..."]
    }
  },

  "district_comparison": {
    "description": "各行政区房价对比",
    "format": {
      "districts": ["海淀", "朝阳", "东城", "..."],
      "avg_prices": [75000, 65000, 70000, "..."],
      "price_changes": [-1.2, -0.8, -1.5, "..."]
    }
  },

  "ring_analysis": {
    "description": "环线房价分析",
    "format": {
      "rings": ["2环", "3环", "4环", "5环", "6环"],
      "prices": [85000, 70000, 55000, 40000, 30000]
    }
  },

  "feature_importance": {
    "description": "机器学习特征重要性",
    "format": {
      "features": ["区域", "环线", "月份", "政策", "..."],
      "importance": [0.35, 0.25, 0.15, 0.12, "..."]
    }
  }
}
